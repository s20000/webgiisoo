		</dependency>

	</dependencies>

	<build>
		<finalName>webgiisoo</finalName>
		<sourceDirectory>src/main/java/core</sourceDirectory>
		<plugins>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>build-helper-maven-plugin</artifactId>
				<version>1.7</version>
				<executions>
					<execution>
						<id>add-source</id>
						<phase>generate-sources</phase>
						<goals>
							<goal>add-source</goal>
						</goals>
						<configuration>
							<sources>
								<source>${basedir}/src/main/java/core</source>
								<source>${basedir}/src/main/java/framework</source>
								<source>${basedir}/src/main/java/3rd</source>
								<source>${basedir}/src/main/webapp/modules/default/model
								</source>
							</sources>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.1</version>
				<configuration>
					<source>1.7</source>
					<target>1.7</target>
				</configuration>
			</plugin>

			<plugin>
				<artifactId>maven-jar-plugin</artifactId>
				<version>2.5</version>
				<executions>
					<execution>
						<id>framework</id>
						<goals>
							<goal>jar</goal>
						</goals>
						<phase>prepare-package</phase>
						<configuration>
							<classifier>framework-${project.version}</classifier>
							<includes>
								<include>**/framework/**</include>
							</includes>
							<outputDirectory>${project.build.directory}/webgiisoo/WEB-INF/lib
							</outputDirectory>
						</configuration>
					</execution>
					<execution>
						<id>default</id>
						<goals>
							<goal>jar</goal>
						</goals>
						<phase>prepare-package</phase>
						<configuration>
							<classifier>default-${project.version}</classifier>
							<includes>
								<include>**/com/giisoo/app/web/**</include>
							</includes>
							<outputDirectory>${project.build.directory}/webgiisoo/modules/default/model
							</outputDirectory>
						</configuration>
					</execution>
					<execution>
						<id>3rd</id>
						<goals>
							<goal>jar</goal>
						</goals>
						<phase>prepare-package</phase>
						<configuration>
							<classifier>3rd-${project.version}</classifier>
							<includes>
								<include>**/net/**</include>
								<include>**/org/**</include>
							</includes>
							<outputDirectory>${project.build.directory}/webgiisoo/WEB-INF/lib
							</outputDirectory>
						</configuration>
					</execution>
					<execution>
						<id>core</id>
						<goals>
							<goal>jar</goal>
						</goals>
						<phase>prepare-package</phase>
						<configuration>
							<classifier>core-${project.version}</classifier>
							<includes>
								<include>**/com/giisoo/core/**</include>
								<include>**/com/giisoo/utils/**</include>
							</includes>
							<outputDirectory>${project.build.directory}/webgiisoo/WEB-INF/lib
							</outputDirectory>
						</configuration>
					</execution>
				</executions>
			</plugin>

			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-war-plugin</artifactId>
				<version>2.4</version>
				<configuration>
					<packagingExcludes>**/classes/**, **/model/com/**
					</packagingExcludes>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>
